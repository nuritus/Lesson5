<Window x:Class="solid49.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:solid49"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name = "button1" Content="button1" HorizontalAlignment="Left" 
                Margin="83,18,0,0" VerticalAlignment="Top" Width="75" Height="32"/>
        <Button x:Name = "button2" Content="button2" HorizontalAlignment="Left" 
                Margin="83,21,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Height="32"/>
        <Button x:Name = "button3" Content="button3" HorizontalAlignment="Left" 
                Margin="83,21,0,0" Grid.Row="2" VerticalAlignment="Top" Width="75" Height="32"/>
        <Button x:Name = "button4" Content="button4" HorizontalAlignment="Left" 
                Margin="83,24,0,0" Grid.Row="3" VerticalAlignment="Top" Width="75" Height="29"/>
    </Grid>
</Window>
